# Telegram Bot Token
BOT_TOKEN=your_bot_token_here

# Ссылка на аккаунт менеджера
MANAGER_URL=https://t.me/your_manager

# ID супергруппы для логирования
LOGGING_SUPERGROUP_ID=-1001234567890

# ID администраторов (через запятую)
ADMIN_IDS=123456789,987654321

# Настройки базы данных (опционально, если не используете docker-compose)
DATABASE_URL=postgresql+asyncpg://bot_user:bot_password@localhost:5432/bot_db
